if showSearch === true
	.row
		include ../building-search/index.jade
#main-content
	if showFavSlider === true
		.row
			.Main-video.embed-responsive.embed-responsive-16by9
				iframe.embed-responsive-item(width='560', height='315', src='https://www.youtube.com/embed/rKcMLfp_-bU', frameborder='0', allowfullscreen='')
		.row
			.FavoriteSlider
	if showServices === true
		.row
			.container.Services
				.row
					h3.title.text-color-base Ofrecemos
				.col-md-9
					each service in ["Asesoria Profesional", "Acompañamiento legal y comercial", "Fotografia Especializada", "15 años de experiencia"]
							.col-md-6.Services-item
								i.fa.fa-check-square-o
								span #{service}
